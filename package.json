    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/testing": "^11.1.11",
    "@swc/core": "^1.15.8",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.3",
    "@types/object-path": "^0.11.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jest": "^30.2.0",
    "jest-mock": "^30.2.0",
